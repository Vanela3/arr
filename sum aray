#include <stdio.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */ 
    int  num,*arr,i, sum=0;
    scanf("%d",&num);
    arr = (int*)malloc(num * sizeof(int));
    for(i=0;i<num;i++){
        scanf("%d",&arr[i]);
        sum+=arr[i];}
        printf("%d\n",sum);
        free(arr);
    return 0;
}
